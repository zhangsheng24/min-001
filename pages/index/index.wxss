.pyg_index .index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.pyg_index .index_swiper swiper image {
  width: 100%;
}
.pyg_index .index_cate {
  display: flex;
}
.pyg_index .index_cate .cateNavList {
  flex: 1;
  padding: 20rpx;
}
.pyg_index .index_cate .cateNavList image {
  width: 100%;
}
.pyg_index .index_floor .floor_group .floor_title {
  padding: 10rpx 0;
}
.pyg_index .index_floor .floor_group .floor_title image {
  width: 100%;
}
.pyg_index .index_floor .floor_group .floor_list {
  overflow: hidden;
}
.pyg_index .index_floor .floor_group .floor_list .imgBox {
  font-size: 0;
  float: left;
  width: 33.333%;
}
.pyg_index .index_floor .floor_group .floor_list .imgBox:nth-last-child(-n+4) {
  height: 27.72960776vw;
  border-left: 10rpx solid #fff;
}
.pyg_index .index_floor .floor_group .floor_list .imgBox:nth-child(2),
.pyg_index .index_floor .floor_group .floor_list .imgBox:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.pyg_index .index_floor .floor_group .floor_list .imgBox image {
  width: 100%;
  height: 100%;
}
